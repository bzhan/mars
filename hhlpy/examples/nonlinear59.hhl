# Nonlinear benchmark, problem 59
# {(x - 4)^2 + y^2 <= 1}
#    t := 0;
#   {x_dot = x+2*y-y^2, y_dot = -y+y^2, t_dot = 1 & t < 10}
#   @invariant(y<=1, 273630+y*((-32671)+81001*y) < 123190*x)
# {!(x < 2 | y > 1)}
pre [(x - 4)^2 + y^2 <= 1];
t := 0; 
{x_dot = x+2*y-y^2, y_dot = -y+y^2, t_dot = 1 & t < 10} 
invariant 
  [y<=1] {dbx} 
  [273630+y*((-32671)+81001*y) < 123190*x] {bc};
post [!(x < 2 | y > 1)];