# Nonlinear benchmark, problem 71
# {x == 1 & y == 1 & z == 1}
#    t := 1;
#   <x_dot = x*y-x*z, y_dot = y*z-y*x, z_dot = z*x-z*y, 
#    t_dot = 1 & t < 10>@invariant(x>0, y>0, z>0)
# {x + 2*y + z^3 > 0}
pre [x == 1] [y == 1] [z == 1];
t := 1; 
{x_dot = x*y-x*z, y_dot = y*z-y*x, z_dot = z*x-z*y, 
 t_dot = 1 & t < 10}
invariant [x>0] {dbx} [y>0] {dbx} [z>0] {dbx};
post [x + 2*y + z^3 > 0];