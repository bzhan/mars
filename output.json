{
    "vehicle_imp": {
        "input": {
            "veh_a": "veh_a"
        },
        "output": {
            "veh_s": "veh_s",
            "veh_l_v": "veh_l_v",
            "veh_w_v": "veh_w_v"
        },
        "category": "abstract",
        "impl": "Simulink",
        "discrete_computation": "./Examples/AADL/CCS/Simulink/vehicle_imp.xml"
    },
    "camera": {
        "output": {
            "img": "img"
        },
        "period": "200ms",
        "compute_execution_time": "20ms..50ms",
        "category": "device",
        "impl": "HCSP",
        "computation": "(outputs[\"camera\"][\"img\"]!-1; wait(0.2))**"
    },
    "radar": {
        "output": {
            "obs_pos_radar": "obs_pos_radar"
        },
        "period": "10ms",
        "compute_execution_time": "6ms..8ms",
        "category": "device",
        "impl": "HCSP",
        "computation": "t := 0; data := 0.0; (if t < 10 then data := 0.0 elif t < 20 then slope := (55 - 35) / (20 - 10); data := 35 + slope * (t - 10) else data := 0.0 endif; outputs[\"radar\"][\"obs_pos_radar\"]!data; wait(0.01); t := t + 0.01)**"
    },
    "actuator": {
        "input": {
            "cmd": "cmd"
        },
        "output": {
            "veh_a": "veh_a"
        },
        "period": "2ms",
        "category": "device",
        "impl": "HCSP",
        "computation": "(inputs[\"actuator\"][\"cmd\"]?cmd; outputs[\"PHY_vehicle_imp\"][\"veh_a\"]!cmd; wait(0.002))**"
    },
    "GPS": {
        "input": {
            "veh_s": "veh_s"
        },
        "output": {
            "veh_pos": "veh_pos"
        },
        "period": "6ms",
        "category": "device",
        "impl": "HCSP",
        "computation": "(inputs[\"PHY_vehicle_imp\"][\"veh_s\"]?data; outputs[\"gps\"][\"veh_pos\"]!data; wait(0.006))**"
    },
    "laser": {
        "input": {
            "veh_l_v": "veh_l_v"
        },
        "output": {
            "laser_valid": "laser_valid",
            "laser_v": "laser_v"
        },
        "period": "10ms",
        "compute_execution_time": "6ms..9ms",
        "category": "device",
        "impl": "HCSP",
        "computation": "(inputs[\"PHY_vehicle_imp\"][\"veh_v_l\"]?data; t := 0; <t_dot = 1 & t < 0.01> |> [] (outputs[\"laser\"][\"laser_valid\"]!1.0 --> (error := 0; x := data + error; outputs[\"laser\"][\"laser_v\"]!(data+error))); <t_dot = 1 & t < 0.01>)**"
    },
    "wheel": {
        "input": {
            "veh_w_v": "veh_w_v"
        },
        "output": {
            "wheel_valid": "wheel_valid",
            "wheel_v": "wheel_v"
        },
        "period": "10ms",
        "compute_execution_time": "6ms..9ms",
        "category": "device",
        "impl": "HCSP",
        "computation": "(inputs[\"PHY_vehicle_imp\"][\"veh_v_w\"]?data; t := 0; <t_dot = 1 & t < 0.01> |> [] (outputs[\"wheel\"][\"wheel_valid\"]!1.0 --> (error := 0; x := data + error; outputs[\"wheel\"][\"wheel_v\"]!(data+error))); <t_dot = 1 & t < 0.01>)**"
    },
    "user_panel": {
        "output": {
            "event": "event"
        },
        "category": "device",
        "impl": "HCSP",
        "discrete_computation": "outputs[\"user_panel\"][\"event\"]!\"inc\"; wait(0.5); outputs[\"user_panel\"][\"event\"]!\"inc\"; wait(0.5); outputs[\"user_panel\"][\"event\"]!\"inc\"; wait(29); outputs[\"user_panel\"][\"event\"]!\"dec\"; outputs[\"user_panel\"][\"event\"]!\"dec\""
    },
    "cpu": {
        "rq_bus": "bus0",
        "scheduling_protocol": "(rms)",
        "category": "processor"
    },
    "bus0": {
        "latency": "3ms..3ms",
        "category": "bus"
    },
    "tran_img": {
        "source": "camera",
        "source_port": "img",
        "target": ["img_acq"],
        "target_port": ["img"],
        "category": "connection",
        "bus": "bus0"
    },
    "tran_obs_pos_radar": {
        "source": "radar",
        "source_port": "obs_pos_radar",
        "target": ["comp_obs_pos"],
        "target_port": ["obs_pos_radar"],
        "category": "connection",
        "bus": "bus0"
    },
    "tran_cmd": {
        "source": ["emerg"],
        "source_port": ["cmd"],
        "target": "actuator",
        "target_port": "cmd",
        "category": "connection",
        "bus": "bus0"
    },
    "tran_veh_a": {
        "source": "actuator",
        "source_port": "veh_a",
        "target": "vehicle_imp",
        "target_port": "veh_a",
        "category": "connection"
    },
    "tran_veh_s": {
        "source": "vehicle_imp",
        "source_port": "veh_s",
        "target": "GPS",
        "target_port": "veh_s",
        "category": "connection"
    },
    "tran_veh_pos": {
        "source": "GPS",
        "source_port": "veh_pos",
        "target": ["emerg"],
        "target_port": ["veh_pos"],
        "category": "connection",
        "bus": "bus0"
    },
    "tran_veh_l_v": {
        "source": "vehicle_imp",
        "source_port": "veh_l_v",
        "target": "laser",
        "target_port": "veh_l_v",
        "category": "connection"
    },
    "tran_laser_valid": {
        "source": "laser",
        "source_port": "laser_valid",
        "target": ["velo_voter"],
        "target_port": ["laser_valid"],
        "category": "connection",
        "bus": "bus0"
    },
    "tran_laser_v": {
        "source": "laser",
        "source_port": "laser_v",
        "target": ["velo_voter"],
        "target_port": ["laser_v"],
        "category": "connection",
        "bus": "bus0"
    },
    "tran_wheel_valid": {
        "source": "wheel",
        "source_port": "wheel_valid",
        "target": ["velo_voter"],
        "target_port": ["wheel_valid"],
        "category": "connection",
        "bus": "bus0"
    },
    "tran_wheel_v": {
        "source": "wheel",
        "source_port": "wheel_v",
        "target": ["velo_voter"],
        "target_port": ["wheel_v"],
        "category": "connection",
        "bus": "bus0"
    },
    "tran_event": {
        "source": "user_panel",
        "source_port": "event",
        "target": ["pan_ctr_th"],
        "target_port": ["event"],
        "category": "connection",
        "bus": "bus0"
    },
    "tran_des_v": {
        "source": ["pan_ctr_th"],
        "source_port": ["des_v"],
        "target": ["emerg"],
        "target_port": ["des_v"],
        "category": "connection"
    }
}